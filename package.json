    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rm -rf dist/*",
        "build:backend": "tsc && tsc-alias",
        "build:frontend": "cd src/frontend && npm run build",
        "copyview": "cd src && find ./ -name *.twig | xargs -I{} cp --parents {} ../dist/",
